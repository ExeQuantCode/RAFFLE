&setup
  task = 2,
  filename_host = "POSCAR_host",
  database_format = "vasprun.xml",
  database = "database",
  seed = 1,
  vps_ratio = 1 0 0,
  bins = 4 4 10
/

&structure
  num_structures=1,
  num_species=2,
  stoichiometry="2 1",
  elements="C Mg"
/

&volume
  vdW=100,
  volvar=10,
  minbond=80,
  maxbond=120
/

&distribution
  cutoff_min = 0.5 0.0 0.0,
  sigma = 0.1 0.1 0.1
/