&setup
  task = 2,
  filename_host = "POSCAR_host",
  database_format = "vasprun.xml",
  seed = 1,
  vps_ratio = 1 0 0,
  bins = 4 4 10
/

&structure
  num_structures=1,
  num_species=2,
  stoichiometry="2 1",
  elements="C Mg"
/

&volume
  vdW=100,
  volvar=10,
  minbond=80,
  maxbond=120
/

&distribution
  c_min=-1,
  c_cut=101,
  sigma_don=0.2,
  sigma_bondlength=0.1,
  enable_self_bonding = .true.
/
