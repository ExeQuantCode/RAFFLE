&setup
  task = 3,
  filename_host = "POSCAR_BaTiO3_intf",
  seed = 1,
  vps_ratio = 0 0 1,
  bins = 4 4 20
/

&structure
  num_structures=1,
  eltot=3,
  stoichiometry_list="1 1 3"
/

&volume
  vdW=100,
  volvar=10,
  minbond=80,
  maxbond=120
/

&distribution
  c_min=-1,
  c_cut=101,
  sigma_don=0.2,
  sigma_bondlength=0.1,
  enable_self_bonding = .true.
/
